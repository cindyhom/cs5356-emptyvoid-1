<% feed.forEach(function(feedItem) { %>
  <article style="margin-bottom: 3em;">
    <div class="card">
      <div class="card-image">
      </div>
      <div class="card-content">
        <div class="media">
          <div class="media-left">
            <figure class="image is-48x48">
              <!-- <img class="is-rounded" src="<%= feedItem.imageSource %>" alt="Placeholder image"> -->
            </figure>
          </div>
          <div class="media-content">
            <p class="title is-4">
            </p>
            <p class="subtitle is-6">
              <%= feedItem.username %>
            </p>
          </div>
        </div>

        <div class="content">
          <%= feedItem.rant %>
        </div>
      </div>
    </div>

  </article>

  <% }) %>
